<body class="is-preload">

  <!-- Wrapper -->
  <div id="wrapper">

    <!-- Main -->
    <div id="main">
      <div class="inner">

        <!-- Header -->
        <header id="header">
          <div class="logo">
            <a href="/">JJob Korea</a>
          </div>
        </header>

        <!-- Banner -->
        <section class="main-banner">
          <div class="container-fluid">
            <div class="row">
              <div class="col-md-12">
                <div class="banner-content">
                  <div class="row">
                    <div class="col-md-12">
                      <div class="banner-caption">
                        <h4>Hello, this is <em>JJob</em> Korea.</h4>
                        <span>Find your job in our website.</span>
                        <p>Check out millions of job and recruitment information and corporate information on JJob
                          Korea.</p>
                        <div class="primary-button">
                          <a href="#">Read Again</a>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </section>

        <!-- Services -->
        <section class="services">
          <div class="container-fluid">
            <div class="row">
              <div class="row">
                <div class="middle">
                  <div class="sub-title">
                    <h3>company support</h3>
                  </div>
                  <div class="support-company">
                    <div class="cmp-list">
                    <% for (var i=0;i<enterList.length; i++) { %>
                      <div class="cmp">
                        <a href="/enterprise-detail/<%= enterList[i].id %>"><img src=<%= enterList[i].image %> class="company-img"></a>
                        <p class="company-name"><%= enterList[i].enterpriseTitle %></p>
                      </div>
                    <% } %>
                    </div>
